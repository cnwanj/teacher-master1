<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.soldier.models.sys.model.TeachingMaterialEntity" table="teaching_material" schema="teacher_files">
        <id name="materialId">
            <column name="material_id" sql-type="bigint(20)"/>
        </id>
        <property name="materialName">
            <column name="material_name" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="press">
            <column name="press" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="publishTime">
            <column name="publish_time" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="remarks">
            <column name="remarks" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="deptId">
            <column name="dept_id" sql-type="bigint(20)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>