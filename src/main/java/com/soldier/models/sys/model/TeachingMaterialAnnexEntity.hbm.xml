<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.soldier.models.sys.model.TeachingMaterialAnnexEntity" table="teaching_material_annex" schema="teacher_files">
        <id name="annexId">
            <column name="annex_id" sql-type="bigint(20)"/>
        </id>
        <property name="filePath">
            <column name="file_path" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="fileName">
            <column name="file_name" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="fileType">
            <column name="file_type" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="materialId">
            <column name="material_id" sql-type="bigint(20)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>